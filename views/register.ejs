<% include ./partials/navbar %>
<div class="header pb-8 pt-5 pt-lg-8 d-flex align-items-center"
  style="min-height: 400px; background-image: url(/assets/img/brand/fikasback.png); background-size: cover; background-position: center top;">
  <!-- Mask -->
  <span class="mask bg-gradient-default opacity-8"></span>
  <!-- Header container -->
  <div class="container-fluid align-items-center">
    <div class="row">
      <div class="col-lg-7 col-md-10">
        <h1 class="display-3 text-white">New Sacco</h1>

      </div>
    </div>
  </div>
</div>
<!-- Page content -->
<div class="container-fluid mt--8">
  <div class="row">

    <div class="col-xl-12 order-xl-1">
      <div class="card bg-secondary shadow">
        <div class="card-header bg-white border-0">
          <div class="row align-items-center">
            <div class="col-8">
              <h3 class="mb-0">New account</h3>
              <% include ./partials/messages %>
            </div>
          </div>
        </div>
        <div class="card-body">
          <form action="/users/saccoadmin" method="POST">
            <h6 class="heading-small text-muted mb-4">User information</h6>
            <div class="pl-lg-4">
              <div class="row">
                <div class="col-lg-6">
                  <div class="form-group">
                    <label class="form-control-label" for="name">Sacco Name</label>
                    <input required autocomplete="off" type="name" id="name" name="name" class="form-control"
                      placeholder="Enter Name" value="<%= typeof name != 'undefined' ? name : '' %>" />
                  </div>
                </div>
                <div class="col-lg-6">
                  <div class="form-group">
                    <label class="form-control-label" for="email">Email address</label>
                    <input required autocomplete="off" type="email" id="email" name="email"
                      class="form-control form-control-alternative" placeholder="email"
                      value="<%= typeof email != 'undefined' ? email : '' %>" />
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-lg-6">
                  <div class="form-group">
                    <%const uniquecode = function saccoCode(length) {
                      const d = new Date;
                      var result = '';
                      var characters = 'ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789';
                      var charactersLength = characters.length;
                      for (var i = 0; i < length; i++) {
                        result += characters.charAt(Math.floor(Math.random() * charactersLength));
                      }
                      return result + d.getFullYear();
                    }%>
                    <label class="form-control-label" for="uniqueSaccoCode">Sacco Code</label>
                    <input readonly type="uniqueSaccoCode" id="uniqueSaccoCode" name="uniqueSaccoCode"
                      class="form-control" placeholder="" value="<%=uniquecode(8)%>" />
                  </div>
                </div>
                <div class="col-lg-6">
                  <div class="form-group">
                    <label class="form-control-label" for="registration_number">Registration Number</label>
                    <input required autocomplete="off" type="number" id="registration_number" name="registration_number"
                      class="form-control form-control-alternative" placeholder="Registration Number"
                      value="<%= typeof registration_number != 'undefined' ? registration_number: '' %>" />
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-lg-6">
                  <div class="form-group">
                    <label class="form-control-label" for="membership">Membership</label>
                    <input required type="text" id="membership" name="membership"
                      class="form-control form-control-alternative" placeholder="membership"
                      value="<%= typeof membership!= 'undefined' ? membership: '' %>" />
                  </div>
                </div>
                <%
                const creatd = function creat(d) {
                  return d.getDate() + "/" + (d.getMonth() + 1) + "/" + d.getFullYear();
                }%>
                <div class="col-lg-6">
                  <div class="form-group">
                    <label class="form-control-label" for="created">Created</label>
                    <input readonly type="text" id="created" name="created"
                      class="form-control form-control-alternative" placeholder="Created" value="<%=creatd(new Date)%>" />
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-lg-6">
                  <div class="form-group">
                    <label class="form-control-label" for="date_founded">Date Founded</label>
                    <input required autocomplete="off" type="text" id="date_founded" name="date_founded"
                      class="form-control form-control-alternative" placeholder="Date Founded"
                      value="<%= typeof date_founded!= 'undefined' ? date_founded: '' %>" />
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-lg-6">
                  <div class="form-group">
                    <label class="form-control-label" for="password">Enter Password</label>
                    <input required autocomplete="off" type="password" id="password" name="password"
                      class="form-control form-control-alternative" placeholder="Enter password"
                      value="<%= typeof password != 'undefined' ? password : '' %>" />
                  </div>
                </div>
                <div class="col-lg-6">
                  <div class="form-group">
                    <label class="form-control-label" for="password2">Confirm Password</label>
                    <input required autocomplete="off" type="password" id="password2" name="password2"
                      class="form-control form-control-alternative" placeholder="Confirm password"
                      value="<%= typeof password2!= 'undefined' ? password2 : '' %>" />
                  </div>
                </div>
              </div>
            </div>
            <hr class="my-4" />
            <!-- Address -->
            <h6 class="heading-small text-muted mb-4">Contact information</h6>
            <div class="pl-lg-4">
              <div class="row">
                <div class="col-md-12">
                  <div class="form-group">
                    <label class="form-control-label" for="input-address">Postal Address</label>
                    <input required autocomplete="off" id="address" name="address"
                      class="form-control form-control-alternative" placeholder="Box Address" value="" type="text" />
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-md-12">
                  <div class="form-group">
                    <label autocomplete="off" class="form-control-label" for="website">Website</label>
                    <input id="website" name="website" class="form-control form-control-alternative"
                      placeholder="Website" value="" type="text" />
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-lg-4">
                  <div class="form-group">
                    <label class="form-control-label" for="postal_code">Postal Code</label>
                    <input required autocomplete="off" type="number" id="postal_code" name="postal_code"
                      class="form-control form-control-alternative" placeholder="postal code" value="" />
                  </div>
                </div>
                <div class="col-lg-4">
                  <div class="form-group">
                    <label class="form-control-label" for="telephone_number">Telephone Number</label>
                    <input required autocomplete="off" type="number" id="telephone_number" name="telephone_number"
                      class="form-control form-control-alternative" placeholder="Telephone Number" value="" />
                  </div>
                </div>
                <div class="col-lg-4">
                  <div class="form-group">
                    <label class="form-control-label" for="status">Status</label>
                    <input readonly type="text" name="status" id="status" class="form-control form-control-alternative"
                      value="Active">
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-lg-4">
                  <div class="form-group">
                    <label class="form-control-label" for="saccoLeaderFname">Sacco Leader's First Name</label>
                    <input required autocomplete="off" type="name" id="saccoLeaderFname" name="saccoLeaderFname"
                      class="form-control form-control-alternative" placeholder="First Name" value="" />
                  </div>
                </div>
                <div class="col-lg-4">
                  <div class="form-group">
                    <label class="form-control-label" for="saccoLeaderLname">Sacco Leader's Last Name</label>
                    <input required autocomplete="off" type="name" id="saccoLeaderLname" name="saccoLeaderLname"
                      class="form-control form-control-alternative" placeholder="Last Name" value="" />
                  </div>
                </div>
                <div class="col-lg-4">
                  <div class="form-group">
                    <label class="form-control-label" for="saccoLeaderPhoneNumber">Sacco Leader's Phone Number </label>
                    <input required autocomplete="off" type="number" id="saccoLeaderPhoneNumber"
                      name="saccoLeaderPhoneNumber" class="form-control form-control-alternative" value="" />
                  </div>
                </div>
              </div>
            </div>
            <hr class="my-4" />
            <!-- Description -->
            <h6 class="heading-small text-muted mb-4">About</h6>
            <div class="pl-lg-4">
              <div class="form-group">
                <label>Description</label>
                <textarea rows="4" class="form-control form-control-alternative"  name="about"
                  placeholder="A few words about the sacco ..."></textarea>
              </div>
            </div>
            <button style="float: right;" class="btn btn-success" type="submit">Submit</button>
          </form>
        </div>
      </div>
    </div>
  </div>
  <!-- Footer -->
  <% include ./partials/footer %>
</div>